{
  "name": "icepaca-backend",
  "version": "1.0.0",
  "description": "ICEPACA E-commerce Backend API",
  "main": "src/server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --inspect=0.0.0.0:9229 src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testNamePattern=\"integration\"",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "migrate": "ts-node scripts/migrate.ts",
    "migrate:rollback": "ts-node scripts/migrate-rollback.ts",
    "seed": "ts-node scripts/seed.ts"
  },
  "keywords": ["ecommerce", "icepaca", "api"],
  "author": "ICEPACA Team",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-facebook": "^3.0.3",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/qrcode": "^1.5.5",
    "@types/sharp": "^0.31.1",
    "@types/speakeasy": "^2.0.10",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "easypost": "^0.1.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.5",
    "nodemon": "^3.1.10",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "qrcode": "^1.5.4",
    "sharp": "^0.34.3",
    "shippo": "^2.15.0",
    "speakeasy": "^2.0.0",
    "stripe": "^18.4.0",
    "ts-node": "^10.9.2",
    "twilio": "^5.8.0",
    "typescript": "^5.9.2",
    "uuid": "^11.1.0",
    "redis": "^4.6.7",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "socket.io": "^4.7.2",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "csurf": "^1.11.0",
    "express-mongo-sanitize": "^2.2.0",
    "xss": "^1.0.14",
    "hpp": "^0.2.3"
  },
  "devDependencies": {
    "@types/express-session": "^1.17.7",
    "@types/csurf": "^1.11.2",
    "@types/compression": "^1.7.2",
    "@types/hpp": "^0.2.2",
    "jest": "^29.6.2",
    "@types/jest": "^29.5.3",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.45.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src"],
    "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/server.ts"
    ]
  }
}
