    "lighthouse": "^11.4.0",
    "@lhci/cli": "^0.12.0",
    "webpagetest": "^0.4.0",
    "size-limit": "^9.0.0",
    "@size-limit/preset-app": "^9.0.0",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "serve": "^14.2.0",
    "cross-env": "^7.0.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
