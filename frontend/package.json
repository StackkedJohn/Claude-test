  "scripts": {
    "start": "react-scripts start",
    "build": "NODE_ENV=production webpack --mode production",
    "build:dev": "NODE_ENV=development webpack --mode development",
    "build:analyze": "ANALYZE=true npm run build",
    "build:fast": "NODE_ENV=production webpack --mode production --optimization-minimize false",
    "serve": "npx serve -s build -l 3000",
    "optimize": "npm run compress-images && npm run generate-sitemap && npm run preload-critical",
    "compress-images": "imagemin src/images/* --out-dir=build/static/images --plugin=imagemin-mozjpeg --plugin=imagemin-pngquant --plugin=imagemin-svgo",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "preload-critical": "node scripts/extract-critical-css.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "eject": "react-scripts eject",
    "lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./reports/lighthouse-report.html --chrome-flags='--headless'",
    "lighthouse:ci": "lhci autorun",
    "performance": "npm run build && npm run serve & sleep 3 && npm run lighthouse && kill %1",
    "performance:mobile": "lighthouse http://localhost:3000 --preset=perf --form-factor=mobile --output html --output-path ./reports/lighthouse-mobile.html",
    "webpagetest": "webpagetest test http://localhost:3000 --key=$WPT_API_KEY",
    "bundle-analyzer": "npx webpack-bundle-analyzer build/static/js/*.js",
    "size-limit": "size-limit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/addon-interactions": "^7.6.0",
    "@storybook/react": "^7.6.0",
    "storybook": "^7.6.0",
